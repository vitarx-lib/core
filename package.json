{
  "name": "vitarx",
  "private": false,
  "version": "1.0.0-beta.40",
  "description": "Vitarx具有活力的响应式前端框架",
  "author": "ZhuChongLin <8210856@qq.com>",
  "license": "MIT",
  "keywords": [
    "vitarx"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vitarx-lib/framework"
  },
  "type": "module",
  "main": "dist/vitarx.umd.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/vitarx.umd.js",
  "jsdelivr": "dist/vitarx.umd.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "node": "./dist/vitarx.umd.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "node": "./index.mjs",
        "default": "./dist/index.js"
      }
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "default": "./dist/jsx-dev-runtime.js"
    },
    "./web-runtime-dom": {
      "types": "./dist/core/renderer/web-runtime-dom/index.d.ts",
      "default": "./dist/core/renderer/web-runtime-dom/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && vite build && tsc && node scripts/index.js",
    "prepublishOnly": "npm run build",
    "push": "npm publish --access=public"
  },
  "dependencies": {
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.7.2",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^6.0.6"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "index.mjs"
  ]
}
